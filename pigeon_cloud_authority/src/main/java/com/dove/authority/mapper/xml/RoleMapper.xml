<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dove.authority.mapper.RoleMapper">

    <select id="getWeightestInRoles" resultType="int">
        SELECT MIN(weight) AS weightest FROM t_authority_role WHERE id IN (
        <foreach collection="list" item="item" index="index" separator=",">
            #{item}
        </foreach>
        )
    </select>


</mapper>
